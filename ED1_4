#include <iostream>
#include<string>
#include <cstdlib>
#include <ctime>
using namespace std;

struct Aluno
{
    int matricula;
    string nome;
    float notaf;
};

int main()
{
    int *x = new int;
    Aluno A;

    setlocale(LC_ALL, "portuguese");

    cout<<"Quantidade de alunos: ";
    cin>> *x;
    int *vet =  new int[*x];

    cout<<"registre os dados dos alunos:"
    for(int i=0; i<(*x); i++){
    
    cout << "Digite sua matricula: ";
    cin >> vet[i].A.matricula;
    cout << "Digite seu nome: ";
    cin >> vet[i].A.nome;
    cout << "Digite sua nota: ";
    cin >> vet[i].A.notaf;

    }


}